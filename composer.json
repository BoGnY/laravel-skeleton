{
    "name": "crypto-technology/laravel-skeleton",
    "description": "Laravel app skeleton.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Crypto Technology srl",
            "email": "admin@cryptotech.srl",
            "homepage": "https://cryptotech.srl",
            "role": "Developer"
        },
        {
            "name": "Luca Bognolo",
            "email": "admin@bogny.eu",
            "homepage": "https://bogny.eu",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/crypto-technology/laravel-skeleton/issues",
        "source": "https://github.com/crypto-technology/laravel-skeleton"
    },
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "artesaos/seotools": "^0.19",
        "creativeorange/gravatar": "^1.0",
        "diglactic/laravel-breadcrumbs": "^6.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/fortify": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/jetstream": "^1.5",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "laravelista/ekko": "^3.2",
        "livewire/livewire": "^2.0",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-cookie-consent": "^2.11",
        "spatie/laravel-csp": "^2.4",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-robots-middleware": "^1.1",
        "spatie/laravel-sitemap": "^5.6",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "master@dev",
        "symplify/changelog-linker": "^8.3",
        "symplify/easy-coding-standard": "^8.3",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('phpcs.xml') || copy('phpcs.xml.dist', 'phpcs.xml');\"",
            "@php -r \"file_exists('.php_cs') || copy('.php_cs.dist', '.php_cs');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('phpcs.xml') || copy('phpcs.xml.dist', 'phpcs.xml');\"",
            "@php -r \"file_exists('.php_cs') || copy('.php_cs.dist', '.php_cs');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan clear-compiled",
            "@php artisan optimize:clear",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "first-run": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link"
        ],
        "update-dev": [
            "@composer update",
            "@php artisan migrate",
            "@php artisan db:seed",
            "@yarn-dev",
            "@update-perm"
        ],
        "update-prod": [
            "@composer install --no-dev",
            "@composer dump-autoload --no-dev --classmap-authoritative",
            "@php artisan migrate --force",
            "@php artisan db:seed --force",
            "@yarn-prod",
            "@update-perm"
        ],
        "reset-db": [
            "@php artisan migrate:fresh --seed --force"
        ],
        "ide-helper": [
            "@composer dump-autoload",
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --write",
            "@php artisan ide-helper:meta"
        ],
        "yarn-dev": [
            "yarn install",
            "yarn dev",
            "@rtlcss-unix"
        ],
        "yarn-prod": [
            "yarn install",
            "yarn prod",
            "@rtlcss-unix"
        ],
        "update-perm": [
            "chown -R www-data:www-data .",
            "chmod -R g+w ."
        ],
        "clear-all": [
            "@php artisan optimize:clear"
        ],
        "test": "@php artisan config:clear && phpunit --configuration phpunit.xml",
        "rtlcss-win": "node_modules\\.bin\\rtlcss.cmd -d public/css",
        "rtlcss-unix": "@php node_modules/.bin/rtlcss -d public/css",
        "ecs-win": "vendor\\bin\\ecs.bat check",
        "ecs-unix": "vendor/bin/ecs check",
        "php-cs-fixer-win": "vendor\\bin\\php-cs-fixer.bat fix --verbose --show-progress=dots --diff",
        "php-cs-fixer-unix": "vendor/bin/php-cs-fixer fix --verbose --show-progress=dots --diff"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "1.0",
            "ext-redis": "1.0"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
