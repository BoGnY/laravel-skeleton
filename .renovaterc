{
    "assignees": ["team:crypto-technology"],
    "baseBranches": ["develop"],
    "extends": [
        "config:base",
        ":automergeDigest",
        ":automergeMinor",
        ":automergeTesters",
        ":combinePatchMinorReleases",
        ":disableRateLimiting",
        ":enableRenovate",
        ":enableVulnerabilityAlertsWithLabel(vulnerability)",
        ":gitSignOff",
        ":ignoreModulesAndTests",
        ":labels(enhancement,fix)",
        ":npm",
        ":preserveSemverRanges",
        ":renovatePrefix",
        ":semanticCommitType(dep-update)",
        ":skipStatusChecks"
    ],
    "reviewers": ["team:crypto-technology"]
}
